import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Long A = sc.nextLong();
        Long B = sc.nextLong();
        System.out.println(fn(A,B));
    }

    public static Long fn(Long A, Long B){
        return ((A+B)*(A-B));
    }
}