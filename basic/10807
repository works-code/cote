import java.util.ArrayList;
import java.util.Scanner;
public class Main {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        ArrayList<Integer> list = new ArrayList<>();
        while(N > 0){
            N--;
            list.add(sc.nextInt());
        }
        int v = sc.nextInt();
        int sum = 0;
        for(int i : list){
            if(i == v){
                sum ++;
            }
        }
        System.out.println(sum);
    }
}