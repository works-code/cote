import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        Long N = sc.nextLong();
        Long M = sc.nextLong();
        Long result = N-M;
        if(result < 0){
            System.out.println(result*-1);
        }else{
            System.out.println(result);
        }
    }
}