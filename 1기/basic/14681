import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Long N = sc.nextLong();
        Long M = sc.nextLong();
        // +,+ 1
        if(N > 0 && M > 0){
            System.out.println(1);
        }else if(N < 0 && M > 0){
            System.out.println(2);
        }else if(N < 0 && M < 0){
            System.out.println(3);
        }else{
            System.out.println(4);
        }
    }
}